<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_test_pic"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.amylilian.text_card.Test_pic"
    android:background="@drawable/bg_test2">

    <ImageView
        android:layout_width="160dp"
        android:layout_height="160dp"
        app:srcCompat="@drawable/pic"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="28dp"
        android:id="@+id/imageView11" />

    <ImageButton
        android:layout_width="160dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/block2"
        android:layout_below="@+id/imageView11"
        android:background="@android:color/transparent"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="14dp"
        android:id="@+id/imageView12"
        android:scaleType="centerCrop" />

    <ImageButton
        android:layout_width="160dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/block2"
        android:layout_below="@+id/imageView12"
        android:background="@android:color/transparent"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:id="@+id/imageView13"
        android:scaleType="centerCrop" />

    <ImageButton
        android:layout_width="160dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/block2"
        android:layout_below="@+id/imageView13"
        android:background="@android:color/transparent"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:id="@+id/imageView14"
        android:scaleType="centerCrop" />

    <ImageButton
        android:layout_width="160dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/block2"
        android:id="@+id/imageView15"
        android:background="@android:color/transparent"
        android:layout_below="@+id/imageView14"
        android:layout_alignStart="@+id/imageView14"
        android:layout_marginTop="12dp"
        android:scaleType="centerCrop" />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:srcCompat="@drawable/btm_right_a"
        android:id="@+id/imageButton2"
        android:background="@android:color/transparent"
        android:scaleType="centerCrop"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView12"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:id="@+id/textView9"
        android:textSize="24sp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView13"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:id="@+id/textView10"
        android:textSize="24sp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView14"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:id="@+id/textView11"
        android:textSize="24sp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView15"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:id="@+id/textView12"
        android:textSize="24sp" />

    <TextView
        android:text="C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:id="@+id/textView20"
        android:textSize="48sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="5dp" />
</RelativeLayout>
